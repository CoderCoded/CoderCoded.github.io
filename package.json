{
  "name": "crcd-contact-mailer",
  "version": "1.0.0",
  "description": "Contact backend for our site",
  "private": true,
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "bundle exec jekyll serve --incremental",
    "checkout": "git clone https://github.com/CoderCoded/CoderCoded.github.io.git --branch master _site",
    "clean": "rm -rf _site/*",
    "prod": "JEKYLL_ENV=production bundle exec jekyll build",
    "build": "npm-run-all clean prod",
    "clone": "npm-run-all clean checkout",
    "release": "cd _site && git add --all && git commit -m \"Release at $(date -u +\"%Y-%m-%dT%H:%M:%SZ\")\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CoderCoded/codercoded.github.io.git"
  },
  "author": "Coder Coded",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CoderCoded/codercoded.github.io/issues"
  },
  "homepage": "https://github.com/CoderCoded/codercoded.github.io#readme",
  "dependencies": {
    "body-parser": "^1.14.1",
    "bunyan": "^1.5.1",
    "config": "^1.16.0",
    "express": "^4.13.3",
    "joi": "^6.9.1",
    "js-yaml": "^3.4.3",
    "nodemailer": "^1.8.0",
    "nodemailer-smtp-transport": "^1.0.3"
  },
  "devDependencies": {
    "eslint": "^6.4.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "npm-run-all": "^4.1.5"
  }
}
